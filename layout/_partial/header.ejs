<header class="header">
  <div class="blog-title">
    <a href="<%- url_for() %>" class="logo"><%= config.title %></a>
  </div>
  <nav class="navbar">
    <ul class="menu">
      <% for ([name, menuItem] of Object.entries(theme.menu)) { %>
        <li class="menu-item">
          <a href="<%- url_for(menuItem) %>" class="menu-item-link menu-item-<%- name.toLowerCase() %>"><%= name %></a>
        </li>
      <% } %>
    </ul>
  </nav>
</header>
